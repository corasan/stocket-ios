# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'

def stocket_pods
  pod 'AppCenter'
  pod 'Firebase/Analytics'
end

target 'Production' do
  use_frameworks!

  # Pods for StocketiOS
  stocket_pods

end

target 'Staging' do
  use_frameworks!

  # Pods for StocketiOS
  stocket_pods

end

target 'Development' do
  use_frameworks!

  # Pods for StocketiOS
  stocket_pods

end
